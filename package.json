  "devDependencies": {
    "@ffflorian/jszip-cli": "^3.0.2",
    "@types/node": "^11.13.8",
    "copy-dir-cli": "^0.0.3",
    "make-dir-cli": "^2.0.0",
    "typescript": "^3.4.5"
